    <!-- --------- -->
    <section id="featured-articles" class="featured-articles">
      <div class="container flow">
        <h2 class="section-title text-center ">My Blog</h2>
        <ul role="list" class="articles__list flow">
          {%- for post in collections.post -%}
          <li>
            <article class="snippet">
              <img src="{{ post.data.image }}" alt="{{ post.data.imageAlt }}" class="snippet__image">
              <h3 class="snippet__title">{{ post.data.title }}</h3>
              <p class="snippet__meta">{{ post.date | postDate }}</p>
              <p class="snippet__body">{{ post.data.description }}</p>
              <a href="{{ post.url }}" class="btn btn--primary">Continue Reading</a>
            </article>
          </li>
          {%- endfor -%}
        </ul>
      </div>
    </section>
